#include<stdio.h>
int main()
{
	int n,i;
	printf("enter a n value:");
	scanf("%d",&n);
	int a[n];
	printf("enter set of elements:\n");
	for(i=0;i<n;i++)
	{
      scanf("%d",&a[i]);
	}
	printf("even numbers:\n");
	for(i=0;i<n;i++)
	{
	if(a[i]%2==0)
	{
		printf("%d\n",a[i]);
	}
    }
	printf("odd numbers:\n");
	for(i=0;i<n;i++)
	{
	if(a[i]%2!=0)
	{
			printf("%d\n",a[i]);
	}
}
	return 0;
}
